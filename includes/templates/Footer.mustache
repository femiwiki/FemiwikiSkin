{{!
  string? html-language
  array data-footer-links
}}

<footer id="mw-footer" class="footer-content">
  {{{html-language}}}
  {{#data-footer-links}}
  <ul id="footer-{{category}}" role="contentinfo">
    {{#links}}
    <li id="footer-{{category}}-{{key}}">{{{link}}}</li>
    {{/links}}
  </ul>
  {{/data-footer-links}}
</footer>
